package com.functional.programming;

import java.util.List;

public class FP01_Structured {

	public static void main(String[] args) {
		List<Integer> of = List.of(2, 3, 8, 5, 7, 1, 4, 6, 9, 10);
		PrintAllNumbersList(of);
	}
	
	private static void PrintAllNumbersList(List<Integer> numList) {
		numList.stream().forEach(FP01_Structured::print);
	}

	public static void print(int num) {
		System.out.println(num);
	}

	/*
	 * private static void PrintAllNumbersList(List<Integer> numList) { for(int
	 * num:numList) { System.out.println(num); } }
	 */
	
	
}
